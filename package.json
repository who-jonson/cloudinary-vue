{
  "name": "cloudinary-vue",
  "version": "2.0.0-alpha.1",
  "scripts": {
    "serve": "INTERNAL_DEPS=1 vue-cli-service serve ./playground/main.js",
    "build": "vue-cli-service lint --fix && npm run build:lib",
    "test": "vue-cli-service test:unit",
    "tdd": "yarn run test --watch --coverage=false",
    "bundlewatch": "bundlewatch --config ./bundlewatch.config.js",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name Cloudinary src/index.js && npm run bundlewatch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs-build -s ./src/stories/assets"
  },
  "main": "dist/Cloudinary.umd.js",
  "unpkg": "dist/Cloudinary.umd.min.js",
  "dependencies": {
    "cloudinary-core": "^2.12.0"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@storybook/addon-actions": "^6.4.1",
    "@storybook/addon-essentials": "^6.4.1",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.1",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/vue": "^6.4.1",
    "@types/jest": "^26.0.24",
    "@vue/cli-plugin-babel": "^5.0.0-rc.1",
    "@vue/cli-plugin-eslint": "^5.0.0-rc.1",
    "@vue/cli-plugin-unit-jest": "^5.0.0-rc.1",
    "@vue/cli-service": "^5.0.0-rc.1",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/server-renderer": "^3.2.23",
    "@vue/test-utils": "^2.0.0-rc.17",
    "babel-jest": "^27.4.2",
    "bundlewatch": "^0.2.6",
    "core-js": "3.19.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.20.0 ",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "jest-html-reporters": "^1.2.1",
    "jstoxml": "^1.4.4",
    "vue": "^3.2.0",
    "vue-loader": "^16.8.3",
    "vue-styleguidist": "^4.43.0",
    "vue-template-compiler": "2.6.10",
    "webpack-node-externals": "^1.7.2"
  },
  "license": "MIT",
  "repository": {
    "github": "https://github.com/who-jonson/cloudinary-vue/"
  }
}
